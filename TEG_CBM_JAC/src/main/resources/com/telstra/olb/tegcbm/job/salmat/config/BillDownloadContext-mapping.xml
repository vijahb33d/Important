<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
  
<mapping>
	<description>Mapping between BillDownloadContext Object and BillDownloadContext XML</description>	
	<class name="com.telstra.olb.tegcbm.billdownload.jms.BillDownloadEventNotification">
		<map-to xml="bill_download_event_notification"/>
		<field name="recordId" type="int">
			<bind-xml name="record_id" node="element"/>
		</field>
		<field name="parentRecordId" type="int">
			<bind-xml name="parent_record_id" node="element"/>
		</field>
		<field name="userId" type="string">
			<bind-xml name="user_id" node="element"/>
		</field>
		<field name="bulkReqestName" type="string">
			<bind-xml name="bulk_reqest_name" node="element"/>
		</field>
		<field name="downloadFormat" type="string">
			<bind-xml name="download_format" node="element"/>
		</field>
		<field name="successful" type="boolean">
			<bind-xml name="successful" node="element"/>
		</field>
	</class>
</mapping>
